  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "functions": {
    "apps/web/src/app/**/*.{js,ts,tsx}": {
      "runtime": "nodejs18.x"
    }
  }
}